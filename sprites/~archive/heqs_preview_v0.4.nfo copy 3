    // assume belly dumps in position 1 in consist are behind a mining hauler (in position 0) and don't need front wheels.
    // otherwise use front wheel sprites
 1850 * 22	 02 01 CD // choose belly dump A sprites according to cargo slot (in cargo translation tt)
                 81 47 00 FF // var 47 in format ccccwwtt where I want tt 
                 03 // range
                  01 00 01 01 // coal
                  08 00 08 08 // iron ore
                  0E 00 0E 0E // copper ore
                 11 00 // default 
 1851 * 22	 02 01 CC  // choose belly dump B sprites according to cargo slot (in cargo translation tt)
                 81 47 00 FF // var 47 in format ccccwwtt where I want tt 
                 03 // range
                  21 00 01 01 // coal
                  28 00 08 08 // iron ore
                  2E 00 0E 0E // copper ore
                 31 00 // default 
 1852 * 14	 02 01 CA  // choose belly dump sprites according to position in consist 
                 81 40 00 FF // var 40 in format 00nnbbff where I want ff 
                 01 // range
                  CD 00 01 01 // vehicle is in position 1 in consist (second vehicle)
                 CC 00 // default 
 1853 * 7	 03 01 01 07  // Action 3
      00  // Number of different cargo types to support  
      CA 00 // default
  // ----------------- end of Belly Dump Trailers for Mining Trucks -------------- //


